	<event type="SERVER_VARIABLE" name="SELECT_SINGU_BOARDER_PRICE">
		<event_param var_name="[SECTOR]_SEA_PRICE"/>
		<event_param var_value="VAR(SINGU_BOARDER_PRICE)"/>
	</event>

	<event type="SERVER_VARIABLE" name="SELECT_SINGU_CAPTAIN">
		<event_param chain_event="SELECT_SINGU_CAPTAIN_PRICE"/>
		<event_param var_name="[SECTOR]_SEA_SELECTED_TYPE"/>
		<event_param var_value="CAPTAIN"/>
	</event>
	<event type="SERVER_VARIABLE" name="SELECT_SINGU_CAPTAIN_PRICE">
