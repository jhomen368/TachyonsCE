	<event type="SERVER_VARIABLE" name="SELECT_SEA_BOARDER">
		<event_param chain_event="SELECT_SEA_BOARDER_PRICE"/>
		<event_param var_name="[SECTOR]_SEA_SELECTED_TYPE"/>
		<event_param var_value="BOARDER"/>
	</event>
	<event type="SERVER_VARIABLE" name="SELECT_SEA_BOARDER_PRICE">
		<event_param var_name="[SECTOR]_SEA_PRICE"/>
		<event_param var_value="VAR(SEA_BOARDER_PRICE)"/>
	</event>

	<event type="SERVER_VARIABLE" name="SELECT_SEA_CAPTAIN">
		<event_param chain_event="SELECT_SEA_CAPTAIN_PRICE"/>
		<event_param var_name="[SECTOR]_SEA_SELECTED_TYPE"/>
		<event_param var_value="CAPTAIN"/>
	</event>
	<event type="SERVER_VARIABLE" name="SELECT_SEA_CAPTAIN_PRICE">
		<event_param var_name="[SECTOR]_SEA_PRICE"/>
		<event_param var_value="VAR(SEA_CAPTAIN_PRICE)"/>
	</event>

	<event type="GROUP" name="GIVE_RANDOM_GUN">
		<condition type="CHANCE" percent="35"/>
		<event_param random_chain_event="1"/>
		<event_param chain_event="GIVE_LASER_RIFLE"/>
		<event_param chain_event="GIVE_PLASMA_PISTOL"/>
	</event>
	<event type="GROUP" name="GIVE_RANDOM_GUN2">
		<condition type="CHANCE" percent="15"/>
		<event_param random_chain_event="1"/>
		<event_param chain_event="GIVE_PLASMA_RIFLE"/>
		<event_param chain_event="GIVE_GRENADE"/>
		<event_param chain_event="GIVE_MINI_ROCKET"/>
	</event>
	<event type="MODIFY_MOB" name="GIVE_LASER_RIFLE">
		<event_param weapon="Laser rifle"/>
	</event>
	<event type="MODIFY_MOB" name="GIVE_PLASMA_PISTOL">
		<event_param weapon="Plasma pistol"/>
	</event>
	<event type="MODIFY_MOB" name="GIVE_PLASMA_RIFLE">
		<event_param weapon="Plasma rifle"/>
	</event>
	<event type="MODIFY_MOB" name="GIVE_GRENADE">
		<event_param weapon="Grenade"/>
	</event>
	<event type="MODIFY_MOB" name="GIVE_MINI_ROCKET">
		<event_param weapon="Mini rocket"/>
	</event>

	<event type="SPAWN_NPC" name="SPAWN_SINGU_HR">
		<spawn_npc>
			<npc_param npc_qty="1"/>
			<npc_param npc_is_crew="1"/>
			<npc_param npc_name="Singularity Head HR"/>
			<npc_param npc_race="HUMAN"/>
			<npc_param npc_home_system="DOOR_CONTROL"/>
			<npc_param npc_spawn_at_home="1"/>
			<npc_param npc_dialogue="HR_AGENT_SINGU"/>
			<npc_param npc_remote_dialogue="HR_AGENT_SINGU"/>
			<npc_ai type="NPC_LIFESUPPORT" heal_at_percent="90" oxygen_at_percent="90"/>
			<npc_ai type="NPC_DEFENDER"/>
			<npc_ai type="NPC_FIREMAN"/>
			<npc_ai type="NPC_REPAIRMAN"/>
			<npc_ai type="NPC_WORKER">
				<npc_ai_param SYSTEMS="SENSORS:MEDICAL:OXYGEN:DOOR_CONTROL:SHIELDS:REACTOR"/>
				<npc_ai_param WORK_TIME="44"/>
			</npc_ai>
		</spawn_npc>
	</event>

